{
	"env": {
		"node": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "prettier", "plugin:node/recommended"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"prettier/prettier": ["error"],
		"no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
	}
}
